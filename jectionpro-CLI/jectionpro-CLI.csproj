      </Compile>
      <Compile Update="InterfaceClasses\CloseCommand-List.cs">
        <DependentUpon>ListCommand.cs</DependentUpon>
      </Compile>
      <Compile Update="InterfaceClasses\DeleteCommand-List.cs">
        <DependentUpon>ListCommand.cs</DependentUpon>
      </Compile>
      <Compile Update="InterfaceClasses\EditCommand-Pin.cs">
